<table> 
    <thead> 
        <th><a href="faculty/company" tabindex="1" class="noVisual">Company</a></th>
        <th>Contact</th>
        <th width="40%">Description</th>
        <th><a href="faculty/date" tabindex="1" class="noVisual">Submission Date</a></th>
        <th><a href="faculty/status" tabindex="1" class="noVisual">Status</a></th>
        <th><a href="faculty/department" tabindex="1" class="noVisual">Department</a></th>
    </thead> 
 
    {{#each projects}} 
        <tr> 
            <td> 
                {{this/name}}
            </td> 
            <td> 
                {{this/phone}} 
            </td> 
            <td class="descrip-text">
                <a href="allinformation/{{this/projectID}}" target="_blank" tabindex="1">
                {{this/Description}}
                </a>
            </td>
            <td>
                {{this/Date}}
            </td>
            <td>
                {{this/pstatus}}
            </td>            
            <td>
                {{this/depName}}
            </td>              
        </tr> 
    {{/each}} 
 
</table>